        <activity
            android:name="com.suki.ui.AccountActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar" />
        <activity
            android:name="com.suki.ui.CartActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar" />
        <activity
            android:name="com.suki.ui.HomeActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar" />
        <activity
            android:name="com.suki.ui.LogisticsActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar" />
        <activity
            android:name="com.suki.ui.TFaccountActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar" />
        <activity
            android:name="com.suki.ui.WWActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar" />
    </application>

</manifest>